$$destdir = "$destdir";
$$inputdir = "$sourcedir";

@distributions = ('bullseye');
@architectures = ('all', 'arm64', 'armhf');
@sections = ('main', 'non-free');

%release = ('origin' => "solidrun/bsp-imx8/v3", 'description' => "SolidRun BSP for i.MX8 v3");

$$gpgkey = $gpgkey if ($gpgkey);
$$gpgpassfile = $gpgpassfile if ($gpgpassfile);
1;
