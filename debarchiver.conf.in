$$destdir = "$destdir";
$$inputdir = "$sourcedir";

@distributions = ('bookworm', 'bullseye', 'buster');
@architectures = ('all', 'arm64', 'armhf');
@sections = ('main', 'non-free');

%release = ('origin' => "$origin", 'description' => "$description");

$$gpgkey = $gpgkey if ($gpgkey);
$$gpgpassfile = $gpgpassfile if ($gpgpassfile);
1;
